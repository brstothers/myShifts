CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE EMPLOYEES(EMPLOYEEID INTEGER NOT NULL PRIMARY KEY,EMPLOYEENAME VARCHAR(20),EMPLOYEEPHONE VARCHAR(10),EMPLOYEEWAGE DOUBLE)
CREATE MEMORY TABLE SCHEDULES(SCHEDULEID VARCHAR(8) NOT NULL PRIMARY KEY,SCHEDULEWEEK VARCHAR(20),SCHEDULEMONTH VARCHAR(20),SCHEDULEYEAR VARCHAR(20))
CREATE MEMORY TABLE SHIFTS(EMPLOYEEID INTEGER NOT NULL,SCHEDULEID VARCHAR(8) NOT NULL,WEEKDAY VARCHAR(3) NOT NULL,STARTTIME DOUBLE,ENDTIME DOUBLE,PRIMARY KEY(EMPLOYEEID, SCHEDULEID, WEEKDAY))
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
